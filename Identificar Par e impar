	class Program
	{
		static void Main(string[] args)
		{
			int valor = Convert.ToInt32(Console.ReadLine());

if((valor % 2) == 0)
    Console.WriteLine("Es Par");
else
    Console.WriteLine("Es Impar");

		}
	}
